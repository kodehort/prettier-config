{
  "name": "@kodehort/prettier-config",
  "version": "3.0.0",
  "description": "A curated set of prettier configurations that we use across our projects and can be used in your projects.",
  "repository": "https://github.com/kodehort/prettier-config",
  "license": "MIT",
  "type": "module",
  "author": "Alistair Stead",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    }
  },
  "main": "./lib/index.js",
  "files": [
    "lib/",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf node_modules",
    "dev": "tsc --watch",
    "release": "changeset publish"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-curly": "^0.2.2",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-tailwindcss": "^0.6.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.7.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "packageManager": "pnpm@9.11.0+sha1.4cd20e68438613738e8f2bc9aece61eaa6b3e649"
}
